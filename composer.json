{
    "name": "cratespace/cratespace",
    "type": "project",
    "description": "Freight space sales platform.",
    "keywords": [
        "freight",
        "space",
        "cratespace"
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.4.9",
        "ext-gmp": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "blade-ui-kit/blade-heroicons": "^0.2.2",
        "blade-ui-kit/blade-icons": "^0.4.5",
        "doctrine/dbal": "^2.10",
        "erusev/parsedown": "^1.7.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hashids/hashids": "^4.0",
        "laravel-validation-rules/credit-card": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "moneyphp/money": "^3.3",
        "pusher/pusher-php-server": "^4.1",
        "stevebauman/location": "^5.2",
        "stevebauman/purify": "^3.0",
        "stripe/stripe-php": "^7.52"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.6",
        "mockery/mockery": "^1.3.3",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^8.5|^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Support/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "vendor/bin/phpunit --verbose --color=always"
        ],
        "test-offline": [
            "vendor/bin/phpunit --verbose --color=always --exclude-group network"
        ]
    }
}
