{
    "name": "cratespace/cratespace",
    "type": "project",
    "description": "A freight space sales platform.",
    "keywords": [
        "freight",
        "space",
        "sales"
    ],
    "license": "proprietary",
    "support": {
        "issues": "https://github.com/cratespace/cratespace/issues",
        "source": "https://github.com/cratespace/cratespace"
    },
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "bacon/bacon-qr-code": "^2.0",
        "dompdf/dompdf": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.4.4",
        "ipinfo/ipinfo": "^2.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.54",
        "laravel/tinker": "^2.5",
        "league/commonmark": "^2.0",
        "league/iso3166": "^3.0",
        "moneyphp/money": "^4.0",
        "pragmarx/google2fa": "^8.0",
        "stripe/stripe-php": "^7.95",
        "symfony/intl": "^5.3",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Support/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
